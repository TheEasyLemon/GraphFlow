  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheEasyLemon/GraphFlow/issues"
  },
  "homepage": "https://github.com/TheEasyLemon/GraphFlow#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.13.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
